{
    "builds" : [
        {
            "src" : "./backend/index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes": [
        {
            "src" : "./frontend/build",
            "dest": "vercel/static"
        }
    ],
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/backend/server.js"
        }
      ]
}